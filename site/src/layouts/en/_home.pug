extends ../main

append vars
    -var lang = "en"
    -basedir = `../${basedir}`
    -var page = pages[lang].find(page => page.id === '')

block meta
    -var metaTitle = page.title
    -var metaDescription = page.description
    -var metaKeywords = page.keywords

block headerlinks
    +Translate({translations:page.translations})

block menu
    +Menu({activePath: page.link})